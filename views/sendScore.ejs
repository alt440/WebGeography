<!--This is an invisible page that stands between the first wrongly answered
      question in exam mode and the gameOver page-->
<html>
<head>
  <meta charset="utf-8">
  <title>You Lost!</title>

  <link rel="stylesheet" src="css/sendScore.css">
  <script src="js/sendScoreOnLoad.js"></script>
</head>
<body>

  <form method="post">
    <!-- for backend need the name attribute-->
    <input type="text" id="scoreText" name="scoreText" class="hidden"></input>
    <input type="text" id="continentChoiceText" name="continentChoiceText"
      class="hidden"></input>
    <input type="text" id="hasFinished" name="hasFinished" class="hidden"></input>
    <input type="submit" id="submit" class="hidden"></input>
  </form>

  <!-- need to put this at the bottom because page loads from top to bottom,
          thus scoreText not already defined from the top. -->
  <script src="js/sendScore.js"></script>
</body>
</html>
